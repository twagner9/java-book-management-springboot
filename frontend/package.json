{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/electron/main.js",
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react-dom": "^19.1.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-modal": "^3.16.3",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:react": "react-scripts build",
    "build:electron": "tsc -p electron/tsconfig.json",
    "build-and-copy": "npm run build:electron && npm run build:react && rm -rf ../src/main/resources/static/* && cp -r build/* ../src/main/resources/static/",
    "copy-build-win": "rimraf ../src/main/resources/static/* && xcopy /E /Y /I build\\* ..\\src\\main\\resources\\static\\",
    "build-and-copy-win": "npm run build:electron && npm run build:react && npm run copy-build-win",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/electron": "^1.6.12",
    "@types/react": "^19.1.6",
    "@types/react-modal": "^3.16.3",
    "electron": "^38.1.0",
    "typescript": "^5.9.2"
  }
}
